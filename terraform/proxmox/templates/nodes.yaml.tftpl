# Re-run `terraform apply` to regenerate

nodes:
%{ for node in control_plane_nodes ~}
  - name: ${node.name}
    address: "${node.address}"
    controller: true
    disk: "/dev/sda"
    mac_addr: "${node.mac_addr}"
    schematic_id: "${schematic_id}"
%{ endfor ~}
%{ for node in worker_nodes ~}
  - name: ${node.name}
    address: "${node.address}"
    controller: false
    disk: "/dev/sda"
    mac_addr: "${node.mac_addr}"
    schematic_id: "${schematic_id}"
%{ endfor ~}
